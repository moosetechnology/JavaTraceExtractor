{
	"vm": {
		"host": "localhost",
		"port": "5006"
	},
	"entrypoint": {
		"className": "com.example.ObjectRefSimulation",
		"methodName": "main",
		"methodArguments": [
			"java.lang.String[]"
		],
		"repBefore": 0
	},
	"endpoint": {
